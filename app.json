{
  "expo": {
    "name": "Bumpr",
    "slug": "bumpr",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "bumpr",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.bumpr.app",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Bumpr uses your location to show nearby people and AR Moments in your area.",
        "NSLocationAlwaysUsageDescription": "Bumpr uses your location to show nearby people and AR Moments in your area.",
        "NSCameraUsageDescription": "Bumpr needs camera access to create AR Moments."
      }
    },
    "android": {
      "package": "com.bumpr.app",
      "adaptiveIcon": {
        "backgroundColor": "#5B7C99",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "CAMERA"
      ]
    },
    "web": {
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#FAFBFC",
          "dark": {
            "backgroundColor": "#0F1115"
          }
        }
      ],
      "expo-web-browser",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Bumpr uses your location to show nearby people and AR Moments."
        }
      ],
      [
        "react-native-vision-camera",
        {
          "cameraPermissionText": "Bumpr needs camera access to scan for faces.",
          "enableCodeScanner": true
        }
      ]
    ],
    "reactCompiler": true
  },
  "extra": {
    "eas": {
      "projectId": "your-project-id"
    }
  }
}